{
    "name": "TabFocus",
    "description": "Auto close tabs after a configured time.",
    "version": "0.1",
    "manifest_version": 3,
    "icons": {
      "16": "icons/icons.webp",
      "48": "icons/icons.webp",
      "128": "icons/icons.webp"
    },
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_icon": "icon_16.png",
      "default_popup": "popup.html"
    }
  }